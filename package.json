{
  "name": "turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "projects/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@tailwindcss/forms": "^0.5.3",
    "antd": "^5.3.3",
    "dayjs": "^1.11.7",
    "immer": "^9.0.21",
    "next": "^13.4.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "serverless-mysql": "^1.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@types/node": "20.4.9",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "^10.4.14",
    "eslint": "8.41.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-turbo": "latest",
    "eslint-plugin-react": "7.31.8",
    "postcss": "^8.4.27",
    "prettier": "latest",
    "tailwindcss": "^3.3.3",
    "turbo": "latest",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.1.0"
}